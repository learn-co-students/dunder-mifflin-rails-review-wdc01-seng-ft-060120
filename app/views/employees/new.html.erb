
<%= form_for @employee do |f| %>
  <%= f.label :first_name, "First Name" %>
  <%= f.text_field :first_name %><br>
  <%= f.label :last_name, "Last Name" %>
  <%= f.text_field :last_name %><br>
  <%= f.label :alias, "Alias" %>
  <%= f.text_field :alias %><br>
  <%= f.label :title, "Title" %>
  <%= f.text_field :title %><br>
  <%= f.label :office, "Office" %>
  <%= f.text_field :office %><br>
  <%= f.label :img_url, "Image_url" %>
  <%= f.text_field :img_url %><br>
  <%= f.label :dog_id %>  
  <%= f.select :dog_id, options_from_collection_for_select(Dog.all, "id", "name") %><br>
  <%= f.submit "Add New Employee"%>
<% end %>