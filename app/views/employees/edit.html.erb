<%= form_for @employee do |f| %>
    <%= f.label 'First Name' %><br>
    <%= f.text_field :first_name %><br>

    <%= f.label 'Last Name' %><br>
    <%= f.text_field :last_name %><br>

    <%= f.label 'Alias' %><br>
    <%= f.text_field :alias %><br>

    <%= f.label 'Title' %><br>
    <%= f.text_field :title %><br>

    <%= f.label 'Office' %><br>
    <%= f.text_field :office %><br>

    <%= f.label 'Dog ID' %><br>
    <%= f.text_field :dog_id %><br>

    <%= f.label 'Image' %><br>
    <%= f.text_field :img_url %><br>

    <%= f.submit 'Submit' %><br>

    <%end %>
   
    
