<h1><%= @employee.full_name %></h1>

<img src="<%= @employee.img_url %>">

<ul>
    <li>Alias: <%= @employee.alias %></li>
    <li>Title: <%= @employee.title %></li>
    <li>Office: <%= @employee.office %></li>
    <li>Dog's Name: <%= @employee.dog.name %></li>
</ul>

<%= link_to "Update Employee", edit_employee_path(params[:id])%>

